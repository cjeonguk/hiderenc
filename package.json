{
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.241",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.34.0",
    "css-loader": "^6.7.1",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "2.7.1",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "name": "hider",
  "version": "1.2.5",
  "description": "GUI tool to encrypt&decrypt files",
  "main": "out/main/main.js",
  "author": "Jeonguk Choi",
  "license": "MIT",
  "repository": "https://github.com/cjeonguk/hider",
  "homepage": "https://github.com/cjeonguk/hider#readme",
  "private": true,
  "scripts": {
    "start:main": "tsc -p ./src/main && swc ./src/main -d out && NODE_ENV=development electron .",
    "start:renderer": "tsc -p ./src/renderer && webpack serve",
    "test": "echo \"No tests available\"",
    "build": "tsc -p ./src/main && tsc -p ./src/renderer && swc ./src/main -d out && webpack --mode=production",
    "dist": "electron-builder -mwl --x64",
    "dist:linux": "electron-builder --linux --x64",
    "dist:win": "electron-builder --win --x64",
    "dist:mac": "electron-builder --mac --x64",
    "dist:mac:arm64": "electron-builder --mac --arm64",
    "dist:mac:both": "electron-builder --mac --x64 --arm64"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.1",
    "crypto-js": "^4.1.1",
    "electron-updater": "^5.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "build": {
    "appId": "com.electron.hider",
    "productName": "Hider",
    "artifactName": "${name}-v${version}-${os}-${arch}.${ext}",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "license": "LICENSE",
      "perMachine": true,
      "oneClick": false,
      "runAfterFinish": false,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": "zip",
      "icon": "build/64x64.png"
    },
    "fileAssociations": {
      "ext": "enc",
      "name": "Encrypted file"
    }
  }
}
